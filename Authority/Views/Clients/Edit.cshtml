@using IdentityServer4.EntityFramework.Entities
@model Client

@{
    ViewData["Title"] = "Create";
}

<h1>Editace klienta</h1>
@if (TempData.ContainsKey("SuccessMessage"))
{
    <div class="alert alert-success">@TempData["SuccessMessage"]</div>
}
@if (TempData.ContainsKey("ErrorMessage"))
{
    <div class="alert alert-danger">@TempData["ErrorMessage"]</div>
}
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <h4>Nové vlastnosti</h4>
            <hr />
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="AbsoluteRefreshTokenLifetime"></label>
                <input asp-for="AbsoluteRefreshTokenLifetime" class="form-control" />
                <span asp-validation-for="AbsoluteRefreshTokenLifetime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AccessTokenLifetime"></label>
                <input asp-for="AccessTokenLifetime" class="form-control" />
                <span asp-validation-for="AccessTokenLifetime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AccessTokenType"></label>
                <input asp-for="AccessTokenType" class="form-control" />
                <span asp-validation-for="AccessTokenType" class="text-danger"></span>
            </div>
            <div class="checkbox">
                <label asp-for="AllowAccessTokensViaBrowser">
                    <input asp-for="AllowAccessTokensViaBrowser" />AllowAccessTokensViaBrowser
                </label>
                <span asp-validation-for="AllowAccessTokensViaBrowser" class="text-danger"></span>
            </div>
            <div class="checkbox">
                <label asp-for="AllowOfflineAccess">
                    <input asp-for="AllowOfflineAccess" />AllowOfflineAccess
                </label>
                <span asp-validation-for="AllowOfflineAccess" class="text-danger"></span>
            </div>
            <div class="checkbox">
                <label asp-for="AllowPlainTextPkce">
                    <input asp-for="AllowPlainTextPkce" />AllowPlainTextPkce
                </label>
                <span asp-validation-for="AllowPlainTextPkce" class="text-danger"></span>
            </div>
            <div class="checkbox">
                <label asp-for="AllowRememberConsent">
                    <input asp-for="AllowRememberConsent" />AllowRememberConsent
                </label>
                <span asp-validation-for="AllowRememberConsent" class="text-danger"></span>
            </div>
            <div class="checkbox">
                <label asp-for="AlwaysIncludeUserClaimsInIdToken">
                    <input asp-for="AlwaysIncludeUserClaimsInIdToken" />AlwaysIncludeUserClaimsInIdToken
                </label>
                <span asp-validation-for="AlwaysIncludeUserClaimsInIdToken" class="text-danger"></span>
            </div>
            <div class="checkbox">
                <label asp-for="AlwaysSendClientClaims">
                    <input asp-for="AlwaysSendClientClaims" />AlwaysSendClientClaims
                </label>
                <span asp-validation-for="AlwaysSendClientClaims" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AuthorizationCodeLifetime"></label>
                <input asp-for="AuthorizationCodeLifetime" class="form-control" />
                <span asp-validation-for="AuthorizationCodeLifetime" class="text-danger"></span>
            </div>
            <div class="checkbox">
                <label asp-for="BackChannelLogoutSessionRequired">
                    <input asp-for="BackChannelLogoutSessionRequired" />BackChannelLogoutSessionRequired
                </label>
                <span asp-validation-for="BackChannelLogoutSessionRequired" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="BackChannelLogoutUri"></label>
                <input asp-for="BackChannelLogoutUri" class="form-control" />
                <span asp-validation-for="BackChannelLogoutUri" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ClientClaimsPrefix"></label>
                <input asp-for="ClientClaimsPrefix" class="form-control" />
                <span asp-validation-for="ClientClaimsPrefix" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ClientId"></label>
                <input asp-for="ClientId" class="form-control" />
                <span asp-validation-for="ClientId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ClientName"></label>
                <input asp-for="ClientName" class="form-control" />
                <span asp-validation-for="ClientName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ClientUri"></label>
                <input asp-for="ClientUri" class="form-control" />
                <span asp-validation-for="ClientUri" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ConsentLifetime"></label>
                <input asp-for="ConsentLifetime" class="form-control" />
                <span asp-validation-for="ConsentLifetime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Description"></label>
                <input asp-for="Description" class="form-control" />
                <span asp-validation-for="Description" class="text-danger"></span>
            </div>
            <div class="checkbox">
                <label asp-for="EnableLocalLogin">
                    <input asp-for="EnableLocalLogin" />EnableLocalLogin
                </label>
                <span asp-validation-for="EnableLocalLogin" class="text-danger"></span>
            </div>
            <div class="checkbox">
                <label asp-for="Enabled">
                    <input asp-for="Enabled" />Enabled
                </label>
                <span asp-validation-for="Enabled" class="text-danger"></span>
            </div>
            <div class="checkbox">
                <label asp-for="FrontChannelLogoutSessionRequired">
                    <input asp-for="FrontChannelLogoutSessionRequired" />FrontChannelLogoutSessionRequired
                </label>
                <span asp-validation-for="FrontChannelLogoutSessionRequired" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FrontChannelLogoutUri"></label>
                <input asp-for="FrontChannelLogoutUri" class="form-control" />
                <span asp-validation-for="FrontChannelLogoutUri" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdentityTokenLifetime"></label>
                <input asp-for="IdentityTokenLifetime" class="form-control" />
                <span asp-validation-for="IdentityTokenLifetime" class="text-danger"></span>
            </div>
            <div class="checkbox">
                <label asp-for="IncludeJwtId">
                    <input asp-for="IncludeJwtId" />IncludeJwtId
                </label>
                <span asp-validation-for="IncludeJwtId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="LogoUri"></label>
                <input asp-for="LogoUri" class="form-control" />
                <span asp-validation-for="LogoUri" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PairWiseSubjectSalt"></label>
                <input asp-for="PairWiseSubjectSalt" class="form-control" />
                <span asp-validation-for="PairWiseSubjectSalt" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ProtocolType"></label>
                <input asp-for="ProtocolType" class="form-control" />
                <span asp-validation-for="ProtocolType" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="RefreshTokenExpiration"></label>
                <input asp-for="RefreshTokenExpiration" class="form-control" />
                <span asp-validation-for="RefreshTokenExpiration" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="RefreshTokenUsage"></label>
                <input asp-for="RefreshTokenUsage" class="form-control" />
                <span asp-validation-for="RefreshTokenUsage" class="text-danger"></span>
            </div>
            <div class="checkbox">
                <label asp-for="RequireClientSecret">
                    <input asp-for="RequireClientSecret" />RequireClientSecret
                </label>
                <span asp-validation-for="RequireClientSecret" class="text-danger"></span>
            </div>
            <div class="checkbox">
                <label asp-for="RequireConsent">
                    <input asp-for="RequireConsent" />RequireConsent
                </label>
                <span asp-validation-for="RequireConsent" class="text-danger"></span>
            </div>
            <div class="checkbox">
                <label asp-for="RequirePkce">
                    <input asp-for="RequirePkce" />RequirePkce
                </label>
                <span asp-validation-for="RequirePkce" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="SlidingRefreshTokenLifetime"></label>
                <input asp-for="SlidingRefreshTokenLifetime" class="form-control" />
                <span asp-validation-for="SlidingRefreshTokenLifetime" class="text-danger"></span>
            </div>
            <div class="checkbox">
                <label asp-for="UpdateAccessTokenClaimsOnRefresh">
                    <input asp-for="UpdateAccessTokenClaimsOnRefresh" />UpdateAccessTokenClaimsOnRefresh
                </label>
                <span asp-validation-for="UpdateAccessTokenClaimsOnRefresh" class="text-danger"></span>
            </div>
            <button type="submit" class="btn btn-primary">Uložit</button> <a class="btn" asp-action="Index">Zpět</a>
        </form>
    </div>
</div>
